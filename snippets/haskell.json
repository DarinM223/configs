{
	"Function": {
		"prefix": "fn",
		"body": [
			"$1 :: $2",
			"$1 = $3"
		]
	},
	"Case": {
		"prefix": "case",
		"body": [
			"case $1 of",
			"\t$2 -> $3"
		]
	},
	"If": {
		"prefix": "if",
		"body": [
			"if $1",
			"\tthen $2",
			"\telse $3"
		]
	},
	"Let": {
		"prefix": "let",
		"body": [
			"let",
			"\t$1 = $2"
		]
	},
	"Data": {
		"prefix": "data",
		"body": [
			"data $1",
			"\t= $2"
		]
	},
	"Newtype": {
		"prefix": "nut",
		"body": "newtype $1 = $1 { un$1 :: $2 }"
	},
	"Class": {
		"prefix": "class",
		"body": [
			"class $1 where",
			"\t$2"
		]
	}
}
