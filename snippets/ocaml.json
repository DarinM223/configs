{
	"Let syntax": {
		"prefix": "let",
		"body": [
			"let $1 in"
		]
	},
	"Let star syntax": {
		"prefix": "lets",
		"body": [
			"let* $1 in"
		]
	},
	"In": {
		"prefix": "in",
		"body": [
			"in\n"
		]
	},
	"With": {
		"prefix": "with",
		"body": [
			"with\n"
		]
	},
	"Do": {
		"prefix": "do",
		"body": [
			"do",
			"\t$1",
			"done"
		]
	},
	"Failwith": {
		"prefix": "fail",
		"body": [
			"failwith \"\""
		]
	}
}
