<!-- Put this in ~/.PyCharmCE/config/keymaps/ -->
<keymap version="1" name="XWin copy" parent="Default for XWin">
  <action id="ExecuteInPyConsoleAction">
    <keyboard-shortcut first-keystroke="shift alt e" />
    <keyboard-shortcut first-keystroke="ctrl alt x" />
  </action>
  <action id="Run Mypy Daemon">
    <keyboard-shortcut first-keystroke="shift ctrl m" />
    <keyboard-shortcut first-keystroke="shift ctrl alt m" />
  </action>
</keymap>
