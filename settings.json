{
    "telemetry.telemetryLevel": "off",
    "vim.easymotion": true,
    "vim.leader": " ",
    "vim.handleKeys": {
        "<C-p>": false,
        "<C-a>": false
    },
    "vim.foldfix": true,
    "files.trimTrailingWhitespace": true,
    "keyboard.dispatch": "keyCode",
    "editor.fontFamily": "DejaVu Sans Mono",
    "editor.fontSize": 14,
    "editor.parameterHints.enabled": false,
    "editor.dragAndDrop": false,
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "*.mutable": {
                "fontStyle": "" // underline is the default
            }
        }
    },
    "editor.folding": true,
    "editor.snippetSuggestions": "inline",
    "editor.tabSize": 2,
    "editor.minimap.showSlider": "always",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "diffEditor.ignoreTrimWhitespace": false,
    "update.mode": "manual",
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "workbench.navigationControl.enabled": false,
    "workbench.colorTheme": "Default Dark+",
    "chat.agent.enabled": false,
    "chat.commandCenter.enabled": false,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "files.readonlyInclude": {
        "**/.cargo/registry/src/**/*.rs": true,
        "**/.cargo/git/checkouts/**/*.rs": true,
        "**/lib/rustlib/src/rust/library/**/*.rs": true
    },
    "[rust]": {
        "editor.tabSize": 4
    },
    "[c]": {
        "editor.tabSize": 4
    },
    "[python]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "[go]": {
        "editor.tabSize": 4
    },
    "julia.execution.resultType": "both",
    "julia.enableCrashReporter": false,
    "julia.enableTelemetry": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "haskell.serverEnvironment": {
        "PATH": "${HOME}/.ghcup/bin:$PATH"
    },
    "haskell.trace.client": "debug",
    "haskell.trace.server": "messages",
    "haskell.manageHLS": "GHCup",
    "haskell.ghcupExecutablePath": "/home/d/.ghcup/bin/ghcup",
    "haskell.plugin.ormolu.config.external": true,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "python.defaultInterpreterPath": "/home/d/Documents/python/leetcode/venv",
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.typeEvaluation.strictSetInference": true,
    "python.analysis.typeEvaluation.strictListInference": true,
    "python.analysis.typeEvaluation.strictDictionaryInference": true,
    "cmake.configureOnOpen": true,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.options.statusBarVisibility": "visible",
    "millet.format.engine": "smlfmt",
    "millet.server.path": "/home/d/Documents/mirrored/millet/target/release/millet-ls",
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.cStandard": "c11"
}